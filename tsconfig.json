{
	"compilerOptions": {
		"target": "ES2019",
		"module": "commonjs",
		"moduleResolution": "node",
		"outDir": "./lib",
		"incremental": true,
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"typeRoots": [
			"./node_modules/@types",
			"./src/@types"
		]
	}
}
